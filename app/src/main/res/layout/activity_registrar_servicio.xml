<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActivityRegistrarServicio">

    <TextView
            android:text="REGISTRO SERVICIO PROVEEDOR"
            android:textAlignment="center"
            android:textSize="24sp"
            android:layout_width="251dp"
            android:layout_height="68dp" android:id="@+id/textView13"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="115dp"
            android:layout_marginEnd="116dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="36dp"
            style="@style/Widget.AppCompat.TextView.SpinnerItem" app:fontFamily="sans-serif-black"
            app:guidelineUseRtl="false" app:layout_wrapBehaviorInParent="included"
            app:layout_constraintHorizontal_bias="0.51"
            android:layout_marginBottom="13dp" app:layout_constraintBottom_toTopOf="@+id/imageView2"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_launcher_foreground"
            android:id="@+id/imageView4"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="151dp"
            android:layout_marginEnd="152dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="220dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toTopOf="@+id/txtServicio" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtServicio"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="49dp"
            app:layout_constraintEnd_toStartOf="@+id/IbtnAgregar" android:layout_marginEnd="23dp"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="398dp"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="?attr/actionModePasteDrawable"
            android:id="@+id/IbtnAgregar" android:layout_marginStart="22dp"
            app:layout_constraintStart_toEndOf="@+id/txtServicio" android:layout_marginEnd="82dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="270dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="269dp" app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="@string/dialogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnAceptarServicio"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="163dp"
            android:layout_marginEnd="145dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="59dp"
            android:layout_marginTop="194dp" app:layout_constraintTop_toBottomOf="@+id/txtServicio"/>
</androidx.constraintlayout.widget.ConstraintLayout>